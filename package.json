{
    "name": "ai-healthcare-search",
    "version": "1.0.0",
    "description": "AI-powered healthcare provider search system with symptom analysis",
    "main": "backend/app.js",
    "scripts": {
      "start": "node backend/app.js",
      "dev": "concurrently \"npm run server\" \"npm run client\"",
      "server": "nodemon backend/app.js",
      "client": "cd frontend && npm start",
      "build": "cd frontend && npm run build",
      "test": "jest --watchAll=false",
      "test:watch": "jest --watchAll",
      "test:coverage": "jest --coverage",
      "lint": "eslint backend/ frontend/src/",
      "lint:fix": "eslint backend/ frontend/src/ --fix",
      "docker:build": "docker build -t healthcare-search .",
      "docker:run": "docker run -p 3001:3001 healthcare-search",
      "deploy": "npm run build && npm run docker:build"
    },
    "keywords": [
      "healthcare",
      "ai",
      "search",
      "openai",
      "medical",
      "providers",
      "symptoms",
      "mern"
    ],
    "author": "Your Name",
    "license": "MIT",
    "dependencies": {
      "express": "^4.18.2",
      "mongoose": "^7.5.0",
      "cors": "^2.8.5",
      "dotenv": "^16.3.1",
      "helmet": "^7.0.0",
      "morgan": "^1.10.0",
      "express-rate-limit": "^6.10.0",
      "compression": "^1.7.4",
      "openai": "^4.8.0",
      "langchain": "^0.0.142",
      "zod": "^3.22.4",
      "axios": "^1.5.0",
      "jsonwebtoken": "^9.0.2",
      "bcryptjs": "^2.4.3",
      "validator": "^13.11.0",
      "winston": "^3.10.0",
      "node-cron": "^3.0.2",
      "lodash": "^4.17.21",
      "moment": "^2.29.4",
      "uuid": "^9.0.0"
    },
    "devDependencies": {
      "nodemon": "^3.0.1",
      "jest": "^29.6.2",
      "supertest": "^6.3.3",
      "concurrently": "^8.2.0",
      "eslint": "^8.47.0",
      "eslint-config-airbnb-base": "^15.0.0",
      "eslint-plugin-import": "^2.28.1",
      "@babel/preset-env": "^7.22.9",
      "@babel/preset-react": "^7.22.5",
      "babel-jest": "^29.6.2"
    },
    "engines": {
      "node": ">=16.0.0",
      "npm": ">=8.0.0"
    },
    "jest": {
      "testEnvironment": "node",
      "setupFilesAfterEnv": ["<rootDir>/backend/test/setup.js"],
      "testMatch": ["**/__tests__/**/*.js", "**/?(*.)+(spec|test).js"],
      "collectCoverageFrom": [
        "backend/**/*.js",
        "!backend/test/**",
        "!backend/node_modules/**"
      ]
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/yourusername/ai-healthcare-search.git"
    },
    "bugs": {
      "url": "https://github.com/yourusername/ai-healthcare-search/issues"
    },
    "homepage": "https://github.com/yourusername/ai-healthcare-search#readme"
  }